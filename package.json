{
  "name": "react-starter-kit",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "0.18.0",
    "bootstrap": "4.3.1",
    "eslint-plugin-prettier": "3.0.1",
    "flow-bin": "0.95.1",
    "husky": "1.3.1",
    "lint-staged": "8.1.5",
    "prettier": "1.16.4",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-redux": "6.0.1",
    "react-router-dom": "4.4.0",
    "react-scripts": "2.1.8",
    "reactstrap": "7.1.0",
    "redux": "4.0.1",
    "redux-form": "8.1.0",
    "redux-thunk": "2.3.0",
    "source-map-explorer": "1.7.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "flow": "flow"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --single-quote --write",
      "git add"
    ]
  }
}
